# Required: OpenAI-compatible OCR endpoint
OPENAI_KEY=
OPENAI_BASE_URL=
MODEL_NAME=

# Required: Mihomo subscription URL
MIHOMO_SUBSCRIPTION_URL=

# Required mailbox config for default provider (MAIL_PROVIDER=vmail)
VMAIL_BASE_URL=
VMAIL_API_KEY=

# Run mode and browser engine
# RUN_MODE=headed # supported: headed|headless
# HEADLESS=false
# BROWSER_ENGINE=chrome
# INSPECT_BROWSER_ENGINE=chrome

# Chrome options
# CHROME_EXECUTABLE_PATH=/Applications/Google Chrome.app/Contents/MacOS/Google Chrome
# CHROME_NATIVE_AUTOMATION=true
# CHROME_IDENTITY_OVERRIDE=false
# CHROME_STEALTH_JS_ENABLED=false
# CHROME_WEBRTC_HARDENED=true
# CHROME_PROFILE_DIR=output/chrome-profile
# CHROME_REMOTE_DEBUGGING_PORT=0
# INSPECT_CHROME_NATIVE=true
# INSPECT_CHROME_PROFILE_DIR=output/chrome-inspect-profile
# INSPECT_KEEP_OPEN_MS=900000

# Runtime tuning
# SLOWMO_MS=50
# MAX_CAPTCHA_ROUNDS=30
# OCR_RETRY_WINDOW_MS=300000
# OCR_INITIAL_COOLDOWN_MS=12000
# OCR_MAX_COOLDOWN_MS=120000
# OCR_REQUEST_TIMEOUT_MS=25000

# Human confirmation gate before signup submit
# HUMAN_CONFIRM_BEFORE_SIGNUP=false
# HUMAN_CONFIRM_TEXT=CONFIRM

# Mailbox provider (default: vmail)
# MAIL_PROVIDER=vmail
# MAIL_POLL_MS=2500
# EMAIL_WAIT_MS=180000

# VMAIL mailbox options
# VMAIL_DOMAIN=mailbox.example.invalid

# DuckMail mailbox options (required only when MAIL_PROVIDER=duckmail)
# DUCKMAIL_BASE_URL=https://mail-api.example.invalid
# DUCKMAIL_API_KEY=dk_your_api_key
# DUCKMAIL_DOMAIN=mailbox.example.invalid

# Reuse existing Tavily account (optional)
# EXISTING_EMAIL=
# EXISTING_PASSWORD=

# API key generation (optional)
# KEY_NAME=
# KEY_LIMIT=1000

# Proxy and network checks
# MIHOMO_API_PORT=9090
# MIHOMO_MIXED_PORT=7890
# PROXY_CHECK_URL=https://www.cloudflare.com/cdn-cgi/trace
# PROXY_CHECK_TIMEOUT_MS=8000
# PROXY_LATENCY_MAX_MS=3000
# IPINFO_TOKEN=

# Browser transparency precheck
# BROWSER_PRECHECK_ENABLED=true
# BROWSER_PRECHECK_STRICT=true
# BROWSER_PRECHECK_CHECK_HOSTING_PROVIDER=false
# REQUIRE_WEBRTC_VISIBLE=false
# VERIFY_HOST_ALLOWLIST=tavily.com,auth.tavily.com,app.tavily.com
# MODE_RETRY_MAX=3
# BROWSER_LAUNCH_RETRY_MAX=3
# NODE_REUSE_COOLDOWN_MS=1800000
# NODE_RECENT_WINDOW=4
# NODE_CHECK_CACHE_TTL_MS=600000
# NODE_SCAN_MAX_CHECKS=40
# NODE_SCAN_MAX_MS=180000
# NODE_DEFER_LOG_MAX=12
# ALLOW_SAME_EGRESS_IP_FALLBACK=false
# CF_PROBE_ENABLED=false
# CF_PROBE_URL=https://ip.skk.moe/
# CF_PROBE_TIMEOUT_MS=12000
# CF_PROBE_CACHE_TTL_MS=1800000

# SQLite task ledger (registration history + risk signals)
# TASK_LEDGER_ENABLED=true
# TASK_LEDGER_DB_PATH=output/registry/signup-tasks.sqlite
# TASK_LEDGER_BUSY_TIMEOUT_MS=5000
# TASK_LEDGER_IP_RATE_LIMIT_COOLDOWN_MS=43200000
# TASK_LEDGER_IP_RATE_LIMIT_MAX=64
# TASK_LEDGER_SUSPICIOUS_COOLDOWN_MS=86400000
# TASK_LEDGER_SUSPICIOUS_MAX=128
# TASK_LEDGER_CAPTCHA_MISSING_COOLDOWN_MS=43200000
# TASK_LEDGER_CAPTCHA_MISSING_MAX=64
# TASK_LEDGER_CAPTCHA_MISSING_THRESHOLD=2
# TASK_LEDGER_INVALID_CAPTCHA_COOLDOWN_MS=28800000
# TASK_LEDGER_INVALID_CAPTCHA_MAX=64
# TASK_LEDGER_INVALID_CAPTCHA_THRESHOLD=3
# ALLOW_RATE_LIMITED_IP_FALLBACK=false
